
@{
	ViewData["Title"] = "Index";
	Layout = null;
}

<!DOCTYPE html>
<html lang="en">

@await Component.InvokeAsync("Head")

<body>
	<div class="header">
		<div class="container">
			@await Component.InvokeAsync("TopAdress")

			@await Component.InvokeAsync("SocialMedia")
			<div class="clearfix"> </div>
		</div>
	</div>

	@await Component.InvokeAsync("Navbar")


	<div class="modal fade" id="myModal1" tabindex="-1" role="dialog">
		<!-- Modal1 -->
		<div class="modal-dialog">
			<!-- Modal content-->
			<div class="modal-content">
				<div class="modal-header">
					<button type="button" class="close" data-dismiss="modal">&times;</button>
					<h4>In Harvest</h4>
					<img src="~/images/banner1.jpg" alt=" " class="img-responsive">
					<h5>ENVIRONMENT & SUSTAINABILITY</h5>
					<p>Lorem Ipsum is simply dummy text of the printing and typesetting industry. Lorem Ipsum has been the industry's standard dummy text ever since the 1500s, when an unknown printer took a galley of type and scrambled it to make a type specimen book. It has survived not only five centuries, but also the leap into electronic typesetting.</p>
				</div>
			</div>
		</div>
	</div>

	@await Component.InvokeAsync("About")

	@await Component.InvokeAsync("Assistance")


	@await Component.InvokeAsync("Team")


	@await Component.InvokeAsync("Announcement")


	@await Component.InvokeAsync("Gallery")


	<div class="contact-agile" id="contact">
		<div class="container">

			@await Component.InvokeAsync("Adress")


			@await Html.PartialAsync("/Views/Home/SendMessage.cshtml")

		</div>
	</div>

	@await Component.InvokeAsync("Map")


	<div class="copy">
		<p>© @DateTime.Now.Year In Harvest . Tüm Hakları Saklıdır | Design by <a href="http://w3layouts.com/">W3layouts</a> </p>
	</div>

	@await Html.PartialAsync("/Views/Home/ScriptPartial.cshtml")


</body>
</html>

