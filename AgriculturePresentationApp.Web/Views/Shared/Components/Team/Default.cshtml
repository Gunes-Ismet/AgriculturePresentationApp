@model List<GetTeamVM>


<div class="team" id="team">
    <div class="container">
        <h3 class="tittle">Ekip Arkadaşlarımız</h3>
        <p class="heading-btm-w3ls">Birbirinden farklı konularda oldukça donanımlı ekip arkadaşlarımız.</p>
        <div class="wthree_team_grids">
            @foreach (var item in Model)
            {

                <div class="col-md-3 wthree_team_grid">
                    <div class="hovereffect">
                        <img src="~/images/team/@item.ImageUrl" alt=" " class="img-responsive" />
                        <div class="overlay align-content-center">
                            <h6>@item.FullName</h6>
                            <div class="rotate">
                                <p class="group1">
                                    <a href="@item.TwitterUrl">
                                        <i class="fa fa-twitter"></i>
                                    </a>
                                    <a href="@item.FacebookUrl">
                                        <i class="fa fa-facebook"></i>
                                    </a>
                                </p>
                                <hr>
                                <hr>
                                <p class="group2">
                                    <a href="@item.InstagramUrl">
                                        <i class="fa fa-instagram"></i>
                                    </a>
                                    <a href="@item.WebSiteUrl">
                                        <i class="fa fa-dribbble"></i>
                                    </a>
                                </p>
                            </div>
                        </div>
                    </div>
                    <h4>@item.FullName</h4>
                    <p>@item.Title</p>
                </div>
            }
            <div class="clearfix"> </div>
        </div>
    </div>
</div>